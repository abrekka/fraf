<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping default-lazy="false">
<class 
    name="no.ica.fraf.model.MrKonti" 
    table="MR_KONTI"
>

    <id
        name="kontoId"
        type="java.lang.Integer"
        column="KONTOID"
    >
    
    </id>

    <property
        name="kontoNr"
        type="java.lang.Integer"
        column="KONTONR"
        length="32"
    />
    <property
        name="konto"
        type="java.lang.String"
        column="KONTO"
        length="50"
    />
    
    <property
        name="kontogruppeId"
        type="java.lang.Integer"
        column="KONTOGRUPPEID"
        length="50"
    />
    

    <!-- Associations -->
  
</class>
</hibernate-mapping>
